{
	"manifest_version": 2,
	"version": "1.0",
	"name": "Lazaro AI",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["main.js"]
		}
	],
	"permissions": ["activeTab"],

	"web_accessible_resources": ["test.jpg"],
	"icons": {
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		},
		"default_title": "Lazaro AI",
		"default_popup": "index.html"
	}
}
